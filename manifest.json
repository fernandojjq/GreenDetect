{
  "manifest_version": 3,
  "name": "Detector de Greenwashing",
  "version": "1.0",
  "description": "Analiza tiendas online y detecta greenwashing en tiempo real.",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}